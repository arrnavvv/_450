class Solution {
    public boolean containsDuplicate(int[] nums) {
        HashMap<Integer,Integer> map=new HashMap<>();
        boolean flag=false;
        for(int x:nums){
            if(map.getOrDefault(x,0)==1){
                flag=true;
                break;
            }else{
                map.put(x,1);
            }
        }
        return flag;
    }
}
