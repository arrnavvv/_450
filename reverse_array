 Scanner scanner=new Scanner(System.in);
        int n=scanner.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++){
            a[i]=scanner.nextInt();
        }
        int i=0,j=n-1;
        while (i<j){
            int temp = a[i];
            a[i]=a[j];
            a[j]=temp;
            i++;j--;
        }
        System.out.println(Arrays.toString(a));
