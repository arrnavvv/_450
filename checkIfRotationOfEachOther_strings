import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner=new Scanner(System.in);
        String s1=scanner.next();
        String s2=scanner.next();String s3=s1+s1;
        if(s3.contains(s2)){
            System.out.println("Yes, they are rotations");
        }
        else{
            System.out.println("No");
        }
    }
}
