class Solution {
    public int removeDuplicates(int[] nums) {
        if(nums.length==0)
            return 0;
        int pos=nums[0];
        int index=1;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=pos){
                nums[index]=nums[i];
                pos=nums[i];
                index++;
            }
        }
        return index;
    }
}
